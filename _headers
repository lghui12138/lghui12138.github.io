/*
  X-Content-Type-Options: nosniff
  Content-Security-Policy: frame-ancestors 'self'; default-src 'self'; script-src 'self' 'unsafe-inline' https://cdnjs.cloudflare.com https://cdn.jsdelivr.net https://vjs.zencdn.net; style-src 'self' 'unsafe-inline' https://cdnjs.cloudflare.com https://vjs.zencdn.net; font-src 'self' https://cdnjs.cloudflare.com; img-src 'self' data: https:; connect-src 'self' https://cdnjs.cloudflare.com https://cdn.jsdelivr.net https://vjs.zencdn.net
  Referrer-Policy: strict-origin-when-cross-origin
  Cache-Control: no-cache, no-store, must-revalidate
  Content-Type: text/html; charset=utf-8
  X-Frame-Options: DENY
  X-XSS-Protection: 1; mode=block
  Permissions-Policy: geolocation=(), microphone=(), camera=()

*.html
  Cache-Control: no-cache, no-store, must-revalidate
  Content-Type: text/html; charset=utf-8
  X-Content-Type-Options: nosniff

*.{js,css}
  Cache-Control: public, max-age=31536000, immutable
  X-Content-Type-Options: nosniff

*.{png,jpg,jpeg,gif,svg,ico,webp}
  Cache-Control: public, max-age=31536000, immutable
  X-Content-Type-Options: nosniff

*.{woff,woff2,ttf,eot}
  Cache-Control: public, max-age=31536000, immutable
  Content-Type: font/woff2; charset=utf-8
  X-Content-Type-Options: nosniff

*.mp4
  Cache-Control: public, max-age=31536000, immutable
  Content-Type: video/mp4

*.pdf
  Cache-Control: public, max-age=31536000, immutable
  Content-Type: application/pdf

*.{zip,rar,7z}
  Content-Type: application/x-msdownload
  Cache-Control: public, max-age=31536000, immutable

/videos/*
  Cache-Control: public, max-age=31536000, immutable
  Content-Type: video/mp4

/data/*
  Cache-Control: public, max-age=31536000, immutable
  Content-Type: application/json

/exercises/*
  Cache-Control: public, max-age=31536000, immutable
  Content-Type: application/json

