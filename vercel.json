{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/fluid_dynamic", "destination": "/fluid_dynamic.html" },
    { "source": "/fluid_dynamic2", "destination": "/fluid_dynamic_2.html" },
    { "source": "/fluid_dynamic3", "destination": "/fluid_dynamic3.html" },
    { "source": "/public/(.*)", "destination": "/public/$1" }
  ],
  "redirects": [
    { "source": "/index", "destination": "/", "permanent": true }
  ],
  "headers": [
    {
      "source": "/public/api/data.json",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ],
  "error": {
    "404": "/public/404-cn.html"
  }
}
