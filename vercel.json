{
  "builds": [
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/fluid_dynamic", "dest": "/fluid_dynamic.html" },
    { "src": "/fluid_dynamic2", "dest": "/fluid_dynamic_2.html" },
    { "src": "/fluid_dynamic3", "dest": "/fluid_dynamic3.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}

