{
  "version": 2,
  "routes": [
    {
      "src": "/$",
      "dest": "/fluid_dynamic.html",  // 根目录直接跳转
      "status": 200
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/$1"  // 正确提供HTML文件
    },
    {
      "src": "/([^/.]+)",  // 匹配无后缀路径
      "dest": "/$1.html",
      "status": 301
    }
  ]
}
